{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "RAILPACK",
    "buildCommand": "npx nx run twenty-server:build --skip-nx-cache"
  },
  "deploy": {
    "startCommand": "npx nx run twenty-server:serve:production",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 120
  }
}
